main{
    float a = 5;
    float b = 2;

    # demo of for loop
    for(float i = 0; i < 3; i++){
        print << 'val of i ' << i << endl;
    }

    # demo of if condition
    if(a>b){
        print << 'still a is greater than b and value of a is' << a << endl;
    }

    # demo of while loop
    while(b<a){
        print << 'still b is greater than a and value of b is' << b << endl;
        b += 1;
    }
}
